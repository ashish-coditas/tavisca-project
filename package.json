    "lit-element": "^2.4.0",
    "ngx-toastr": "^13.1.0",
    "request": "^2.88.2",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "ts-jest": "^26.4.1",
    "tslib": "^2.0.0",
    "underscore": "^1.11.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/cli": "~10.0.1",
