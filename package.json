{
  "name": "tavsica-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve & npx json-server db.json -m ./node_modules/json-server-auth --port 8080 & npx json-server --watch db.json",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "jest",
    "test:coverage": "jest --coverage --verbose",
    "docs:json": "compodoc -p ./tsconfig.base.json -e json -d .",
    "storybook": "yarn docs:json && start-storybook -p 6006 --no-dll",
    "build-storybook": "yarn docs:json && build-storybook --no-dll"
  },
  "private": true,
